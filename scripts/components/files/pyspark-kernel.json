{
  "display_name": "PySpark",
  "language": "python",
  "argv": [ "/usr/bin/python", "-m", "ipykernel", "-f", "{connection_file}" ],
  "env": {
    "SPARK_HOME": "/usr/local/spark",
    "PYSPARK_PYTHON": "/usr/bin/python",
    "PYTHONPATH": "/usr/local/spark/python:/usr/local/spark/python/lib/py4j-0.9-src.zip",
    "PYTHONSTARTUP": "/usr/local/spark/python/pyspark/shell.py",
    "PYSPARK_SUBMIT_ARGS": "--master local[*] --jars /usr/local/spark/lib/spark-examples-1.6.1-hadoop2.6.0.jar pyspark-shell"
  }
}